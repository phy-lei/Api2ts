{
  "name": "Api2ts",
  "displayName": "Api2ts",
  "description": "http response transform ts",
  "version": "1.0.0",
  "publisher": "phy-lei",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.61.0"
  },
  "keywords": [
    "types",
    "util",
    "typescript",
    "json"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/phy-lei/Api2ts"
  },
  "author": "https://github.com/phy-lei",
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Api2ts.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "Api2ts.codeSelection",
        "title": "Selection",
        "category": "Cat Coding"
      }
    ],
    "keybindings": [
      {
        "command": "Api2ts.codeSelection",
        "key": "alt+Q"
      }
    ],
    "configuration": {
      "title": "Api2ts configuration",
      "properties": {
        "Api2ts.configFile": {
          "type": "string",
          "default": "Api2ts.config.json",
          "description": "choose your Api2ts config file"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "json-to-ts": "^1.7.0"
  }
}
